<sbe:schema xmlns:sbe="http://fixprotocol.io/2016/sbe">
    <sbe:message name="Quote" id="1">
        <sbe:field name="publisherId" id="1" type="int64"/>
        <sbe:field name="timestamp" id="2" type="int64"/>
        <sbe:field name="symbol" id="3" type="string" length="75"/>
        <sbe:field name="isIndicative" id="4" type="bool"/>
        <sbe:field name="bidSize" id="5" type="int32"/>
        <sbe:field name="bidPrice" id="6" type="float"/>
        <sbe:field name="askPrice" id="7" type="float"/>
        <sbe:field name="askSize" id="8" type="int32"/>
    </sbe:message>

    <sbe:message name="Order" id="2">
        <sbe:field name="publisherId" id="1" type="int64"/>
        <sbe:field name="timestamp" id="2" type="int64"/>
        <sbe:field name="clOrdId" id="3" type="int64"/>
        <sbe:field name="side" id="4" type="char"/>
        <sbe:field name="quantity" id="5" type="int32"/>
        <sbe:field name="price" id="6" type="float"/>
        <sbe:field name="timeInForce" id="7" type="char"/>
    </sbe:message>

    <sbe:message name="OrderResponse" id="3">
        <sbe:field name="publisherId" id="1" type="int64"/>
        <sbe:field name="timestamp" id="2" type="int64"/>
        <sbe:field name="side" id="3" type="char"/>
        <sbe:field name="quantity" id="4" type="int32"/>
        <sbe:field name="price" id="5" type="float"/>
        <sbe:field name="filledQuantity" id="6" type="int32"/>
        <sbe:field name="filledPrice" id="7" type="float"/>
    </sbe:message>
</sbe:schema>
